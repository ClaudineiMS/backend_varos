generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "sqlite"
  url      = env("DATABASE_URL")
}

model User {
  id           Int      @id @default(autoincrement())
  nome         String
  email        String   @unique
  telefone     String
  cpf          String   @unique
  idade        Int
  endereco     String
  criadoEm     DateTime @default(now())
  atualizadoEm DateTime @updatedAt
}
